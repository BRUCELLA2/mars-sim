<?xml version="1.0" ?>

<people-configuration>

	<!-- The average amount of oxygen (kg) breathed by a person each Martian Sol -->
	<!-- Default: 1.0 -->
	<oxygen-consumption-rate value="1.0" />

	<!-- The average amount of water (kg) drank by a person each Martian Sol -->
	<!-- Default: 4.0 -->
	<water-consumption-rate value="4.0" />

	<!-- The average amount of food (kg) eaten by a person each Martian Sol -->
	<!-- Default: 1.5 -->
	<food-consumption-rate value="1.5" />

	<!-- Number of Earth minutes a person can survive without oxygen -->
	<!-- Default: 5.0 -->
	<oxygen-deprivation-time value="5.0" />

	<!-- Number of Earth minutes a person can survive without water -->
	<!-- Default: 8640, 6 days -->
	<water-deprivation-time value="8640.0" />

	<!-- Number of Earth minutes a person can survive without food -->
	<!-- Default: 30240.0, 21 days -->
	<food-deprivation-time value="30240.0" />

	<!-- Minimum air pressure (atm) a person can withstand without harm -->
	<!-- Default: .25 atm -->
	<min-air-pressure value=".25" />

	<!-- Number of Earth seconds a person can survive in decompression -->
	<!-- Default: 90.0 seconds -->
	<decompression-time value="90.0" />

	<!-- Minimum temperature (degrees celsius) a person can withstand without harm -->
	<!-- Default: 0 degrees celsius -->
	<min-temperature value="0.0" />

	<!-- Maximum temperature (degrees celsius) a person can withstand without harm -->
	<!-- Default: 48 degrees celsius -->
	<max-temperature value="48.0" />
	
	<!-- Number of Earth minutes a person can survive below minimum temperature -->
	<!-- Default: 240 minutes (4 hours) -->
	<freezing-time value="240.0" />

