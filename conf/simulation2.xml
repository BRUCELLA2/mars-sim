<?xml version="1.0" ?>

<simulation-configuration>

	<!-- Time related configuration -->
	<time-configuration>

		<!-- Simulation time ratio to real time -->
		<!-- Default: 1000.0 -->
		<time-ratio value="1000.0" />
		
		<!-- Earth start date/time -->
		<!-- "MM/dd/yyyy hh:mm:ss" format -->
		<!-- Default: 06/18/2015 07:22:10 -->
		<earth-start-date-time value="06/18/2015 07:22:10" />
		
		<!-- Mars start date/time -->
		<!-- "orbit-month-sol:millisol" format -->
		<!-- Default: 15-Adir-01:000.000 -->
		<mars-start-date-time value="15-Adir-01:000.000" />
		
	</time-configuration>	

</simulation-configuration>