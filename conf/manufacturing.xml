<!-- List of all manufacturing processes. -->
<manufacturing-processes>

	<!-- Process for extracting iron oxide from regolith. -->
	<!-- tech - The minimum workshop tech level required to perform the process. (see tech_level.txt) -->
	<!-- skill - The minimum person skill level in materials science to perform the process. -->
	<!-- work-time - The effort time (in millisols) required to perform the process.  Higher skilled -->
	<!--             workers will produce more effort time than lower skilled workers. -->
	<!-- process-time - The minimum time (in millisols) required for the process regardless of effort time. -->
	<process name="extract iron oxide from regolith" tech="1" skill="1" work-time="100" process-time="0">
		
		<!-- List of inputs for the process (resources and parts). -->
		<inputs>
		
			<!-- amount - The amount of the resource in kilograms. -->
			<resource name="regolith" amount="50.0" />
		</inputs>
		
		<!-- List of outputs from the process (resources, parts, equipment and vehicles). -->
		<outputs>
			<resource name="iron oxide" amount="7.0" />
			<resource name="sand" amount="40.0" />
		</outputs>
	</process>
	
	<process name="extract aluminum oxide from regolith" tech="1" skill="1" work-time="100" process-time="0">
		<inputs>
			<resource name="regolith" amount="50.0" />
		</inputs>
		<outputs>
			<resource name="aluminum oxide" amount="2.0" />
		</outputs>
	</process>
	
	<process name="extract aluminum oxide from sand" tech="1" skill="1" work-time="100" process-time="0">
		<inputs>
			<resource name="sand" amount="50.0" />
		</inputs>
		<outputs>
			<resource name="aluminum oxide" amount="2.0" />
		</outputs>
	</process>
	
	<!-- Fe2O3 + 3CO -> 2Fe + 3CO2 -->
	<process name="carbon monoxide iron reduction" tech="1" skill="1" work-time="50" process-time="200">
		<inputs>
			<resource name="iron oxide" amount="50.0" />
			<resource name="carbon monoxide" amount="26.4" />
		</inputs>
		<outputs>
			<resource name="iron powder" amount="34.9" />
			<resource name="carbon dioxide" amount="41.3" />
		</outputs>
	</process>
	
	<!-- Fe2O3 + 3H2 -> 2Fe + 3H2O -->
	<process name="hydrogen iron reduction" tech="1" skill="1" work-time="50" process-time="200">
		<inputs>
			<resource name="iron oxide" amount="50.0" />
			<resource name="hydrogen" amount="2.5" />
		</inputs>
		<outputs>
			<resource name="iron powder" amount="34.9" />
			<resource name="water" amount="16.9" />
		</outputs>
	</process>
	
	<!-- Produces 5kg iron ingot. -->
	<process name="iron ingot casting" tech="1" skill="2" work-time="100" process-time="200">
		<inputs>
			<resource name="iron powder" amount="25.0" />
		</inputs>
		<outputs>
			<part name="iron ingot" number="5" />
		</outputs>
	</process>
	
	<!-- Produces 1m x 2m x 1mm (15kg) sheet of iron. -->
	<process name="iron sheet manufacturing" tech="2" skill="2" work-time="200" process-time="200">
		<inputs>
			<resource name="iron powder" amount="15.0" />
		</inputs>
		<outputs>
			<part name="iron sheet" number="1" />
		</outputs>
	</process>
	
	<!-- Produces 1m x 2m x 1mm (15kg) sheet of iron. -->
	<process name="iron sheet rolling" tech="2" skill="2" work-time="200" process-time="200">
		<inputs>
			<part name="iron ingot" number="3" />
		</inputs>
		<outputs>
			<part name="iron sheet" number="1" />
		</outputs>
	</process>
	
	<!-- Al2O3 + 3C -> 2Al + 3CO -->
	<!-- Produces 1kg ingot of aluminum. -->
	<process name="aluminum ingot casting" tech="1" skill="2" work-time="100" process-time="200">
		<inputs>
			<resource name="aluminum oxide" amount="9.5" />
			<resource name="carbon" amount="3.0" />
		</inputs>
		<outputs>
			<resource name="carbon monoxide" amount="7.5" />
			<part name="aluminum ingot" number="5" />
		</outputs>
	</process>
	
	<!-- Al2O3 + 3C -> 2Al + 3CO -->
	<!-- Produces 1m x 2m x 1mm (5kg) sheet of aluminum. -->
	<process name="aluminum sheet manufacturing" tech="2" skill="2" work-time="200" process-time="200">
		<inputs>
			<resource name="aluminum oxide" amount="9.5" />
			<resource name="carbon" amount="3.0" />
		</inputs>
		<outputs>
