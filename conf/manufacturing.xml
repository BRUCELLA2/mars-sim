<?xml version="1.0" ?>

<manufacturing-processes>

	<process name="extract hematite from regolith" tech="1" skill="1" work-time="100" process-time="0">
		<inputs>
			<resource name="regolith" amount="50.0" />
		</inputs>
		<outputs>
			<resource name="hematite" amount="7.0" />
			<resource name="sand" amount="40.0" />
		</outputs>
	</process>
	
	<!-- Fe2O3 + 3CO -> 2Fe + 3CO2 -->
	<process name="carbonyl process" tech="1" skill="1" work-time="50" process-time="100">
		<inputs>
			<resource name="hematite" amount="50.0" />
			<resource name="carbon monoxide" amount="26.4" />
		</inputs>
		<outputs>
			<resource name="iron powder" amount="34.9" />
			<resource name="carbon dioxide" amount="41.3" />
		</outputs>
	</process>
	
	<!-- Fe2O3 + 3H2 -> 2Fe + 3H2O -->
	<process name="hydrogen iron reduction" tech="1" skill="1" work-time="50" process-time="100">
		<inputs>
			<resource name="hematite" amount="50.0" />
			<resource name="hydrogen" amount="2.5" />
		</inputs>
		<outputs>
			<resource name="iron powder" amount="34.9" />
			<resource name="water" amount="16.9" />
		</outputs>
	</process>
	
	<process name="iron ingot casting" tech="2" skill="2" work-time="100" process-time="100">
		<inputs>
			<resource name="iron powder" amount="50.0" />
		</inputs>
		<outputs>
			<part name="iron ingot" number="5" />
		</outputs>
	</process>
	
	<!-- CH4 -> C + 2H2 -->
	<process name="pyrolyze reaction" tech="1" skill="1" work-time="25" process-time="50">
		<inputs>
			<resource name="methane" amount="1.3" />
		</inputs>
		<outputs>
			<resource name="carbon" amount="1.0" />
			,<!-- <resource name="hydrogen" amount=".3" /> -->
		</outputs>
	</process>
	
	<process name="steel ingot alloying" tech="2" skill="2" work-time="100" process-time="50">
		<inputs>
			<part name="iron ingot" number="5" />
			<resource name="carbon" amount="1.0" />
		</inputs>
		<outputs>
			<part name="steel ingot" number="5" />
		</outputs>
	</process>
	
	<process name="steel pipe manufacturing" tech="2" skill="2" work-time="200" process-time="100">
		<inputs>
			<part name="steel ingot" number="5" />
		</inputs>
		<outputs>
			<part name="steel pipe" number="5" />
		</outputs>
	</process>
	
	<process name="steel canister manufacturing" tech="2" skill="2" work-time="250" process-time="100">
		<inputs>
			<part name="steel ingot" number="5" />
		</inputs>
		<outputs>
			<part name="steel canister" number="5" />
		</outputs>
	</process>
	
	<process name="gas canister manufacturing" tech="2" skill="3" work-time="100" process-time="0">
		<inputs>
			<part name="steel canister" number="1" />
			<part name="valve" number="2" />
			<part name="gasket" number="4" />
		</inputs>
		<outputs>
			<equipment name="gas canister" number="1" />
		</outputs>
	</process>
	
	<process name="co2 extinguisher manufacturing" tech="2" skill="3" work-time="100" process-time="0">
		<inputs>
			<part name="steel canister" number="1" />
			<part name="valve" number="2" />
			<part name="gasket" number="4" />
			<part name="plastic tubing" number="1" />
			<resource name="carbon dioxide" amount="10.0" />
		</inputs>
		<outputs>
			<part name="co2 extinguisher" number="1" />
		</outputs>
	</process>
	
	<process name="valve manufacturing" tech="2" skill="3" work-time="200" process-time="100">
		<inputs>
			<part name="steel ingot" number="1" />
		</inputs>
		<outputs>
			<part name="valve" number="50" />
		</outputs>
	</process>
	
	<process name="satellite dish manufacturing" tech="2" skill="3" work-time="200" process-time="100">
		<inputs>
			<part name="steel ingot" number="1" />
			<part name="electrical wire" number="10" />
			<part name="wire connector" number="10" />
		</inputs>
		<outputs>
			<part name="satellite dish" number="1" />
		</outputs>
	</process>

</manufacturing-processes>