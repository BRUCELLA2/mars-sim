	<!-- Spark plug insulator uses aluminum oxide ceramic.  I'm not sure how to produce this, so I'm -->
	<!-- using aluminum oxide, lime and sand.  We may need to improve this later.  -->
	<process name="spark plug manufacturing" tech="2" skill="2" work-time="200" process-time="200">
		<inputs>
			<resource name="aluminum oxide" amount=".5" />
			<resource name="lime" amount="2.0" />
			<resource name="sand" amount="2.5" />
			<part name="steel ingot" number="1" />
