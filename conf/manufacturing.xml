	<!-- Gypsum is CaSO4 * 2H2O -->
	<!-- Going with 20% gypsum in regolith.  Not sure if this is good estimate or not. -->
	<process name="extract gypsum from sand" tech="3" skill="2" work-time="100" process-time="0">
		<inputs>
			<resource name="sand" amount="50.0" />
		</inputs>
		<outputs>
			<resource name="gypsum" amount="10.0" />
		</outputs>
	</process>

	<!-- Process for reducing gypsum to calcium carbonate using carbon. -->
	<!-- Wrapping two processes: -->
	<!-- CaSO4*2H2O (gypsum) + 2C -> CaS + 2CO2 + 2H2O -->
	<!-- CaS + H2O + CO2 -> CaCO3 (calcium carbonate) + H2S -->
	<!-- Note: ignoring hydrogen sulfide as waste for now. -->
	<process name="reduce gypsum to calcium carbonate with carbon" tech="3" skill="2" work-time="100" process-time="200">
		<inputs>
			<resource name="gypsum" amount="50.0" />
			<resource name="carbon" amount="7.0" />
		</inputs>
		<outputs>
			<resource name="calcium carbonate" amount="29.0" />
			<resource name="carbon dioxide" amount="12.7" />
			<resource name="water" amount="5.2" />
		</outputs>
	</process>
	
	<!-- Process for reducing gypsum to calcium carbonate using carbon monoxide. -->
	<!-- Wrapping two processes: -->
	<!-- CaSO4*2H2O (gypsum) + 4CO -> CaS + 4CO2 + 2H2O -->
	<!-- CaS + H2O + CO2 -> CaCO3 (calcium carbonate) + H2S -->
	<!-- Note: ignoring hydrogen sulfide as waste for now. -->
	<process name="reduce gypsum to calcium carbonate with carbon monoxide" tech="3" skill="2" work-time="100" process-time="200">
		<inputs>
			<resource name="gypsum" amount="50.0" />
			<resource name="carbon monoxide" amount="32.5" />
		</inputs>
		<outputs>
			<resource name="calcium carbonate" amount="29.0" />
			<resource name="carbon dioxide" amount="38.1" />
			<resource name="water" amount="5.2" />
		</outputs>
	</process>
	
	<!-- Process for reducing gypsum to calcium carbonate using hydrogen. -->
	<!-- Wrapping two processes: -->
	<!-- CaSO4*2H2O (gypsum) + 4H2 -> CaS + 6H2O -->
	<!-- CaS + H2O + CO2 -> CaCO3 (calcium carbonate) + H2S -->
	<!-- Note: ignoring hydrogen sulfide as waste for now. -->
	<process name="reduce gypsum to calcium carbonate with hydrogen" tech="3" skill="2" work-time="100" process-time="200">
		<inputs>
			<resource name="gypsum" amount="50.0" />
			<resource name="hydrogen" amount="2.3" />
			<resource name="carbon dioxide" amount="12.8" />
		</inputs>
		<outputs>
			<resource name="calcium carbonate" amount="29.0" />
			<resource name="water" amount="26.0" />
		</outputs>
	</process>
	
	<!-- CaCO3 (calcium carbonate) -> CaO (lime) + CO2 -->
	<process name="baking calcium carbonate to lime" tech="3" skill="2" work-time="50" process-time="400">
		<inputs>
			<resource name="calcium carbonate" amount="50.0" />
		</inputs>
		<outputs>
			<resource name="lime" amount="27.8" />
			<resource name="carbon dioxide" amount="21.8" />
		</outputs>
	</process>
	
	<!-- Wrapping four processes: -->
	<!-- 3C + CaO -> CaC2 + CO -->
	<!-- CaC2 + H2O -> C2H2 (acetylene) + CaO -->
	<!-- 3C2H2 (acetylene) -> (condensed) C6H6 (benzene) -->
	<!-- C6H6 (benzene) + C2H4 (ethylene) -> C2H3C6H5 (styrene) -->
	<process name="styrene production" tech="3" skill="3" work-time="100" process-time="400">
		<inputs>
			<resource name="carbon" amount="32.12" />
			<resource name="lime" amount="2.0" />
			<resource name="water" amount="16.05" />
			<resource name="ethylene" amount="8.34" />
		</inputs>
		<outputs>
			<resource name="carbon monoxide" amount="24.97" />
			<resource name="styrene" amount="30.95" />
		</outputs>
	</process>
	
